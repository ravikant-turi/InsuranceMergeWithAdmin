<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.infinite.jsf.admin.model.PasswordHistory" table="Admin_Password_History">
        <id name="id" column="id">
            <generator class="native"/>
        </id>

        <many-to-one name="user" class="com.infinite.jsf.admin.model.User" column="user_id" not-null="true"/>
        <property name="passwordHash" column="password_hash" type="string"/>
        <property name="changedAt" column="changed_at" type="timestamp" insert="false" update="false"/>
    </class>
</hibernate-mapping>
